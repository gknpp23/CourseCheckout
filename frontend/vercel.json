{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/frontend/browser"
      }
    }
  ],
  "routes": [
    
    {
      "src": "/styles.([a-f0-9]{8,20})\\.css",
      "headers": {
        "Content-Type": "text/css",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    
    {
      "src": "/(.*\\.(png|jpg|jpeg|svg|webp|gif|ico))",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}