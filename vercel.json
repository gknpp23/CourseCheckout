{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist/browser"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/browser/index.csr.html"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/frontend/dist/browser/browser/$1"
    }
  ]
}
